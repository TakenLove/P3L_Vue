-md mb-0>\r\n                <h2 class=\"text-md-center\">Layanan</h2>\r\n                <v-layout row wrap style=\"margin:10px\">\r\n                    <v-flex xs6 class=\"text-right\">\r\n                        <v-text-field v-model=\"keyword\" append-icon=\"mdi-search-web\" label=\"Search\" hide-details>\r\n                        </v-text-field>\r\n                    </v-flex>\r\n                </v-layout>\r\n\r\n                <v-data-table :headers=\"headers\" :items=\"services\" :search=\"keyword\" :loading=\"load\" v-if=\"!showable\">\r\n                    <template v-slot:body=\"{ items }\">\r\n                        <tbody>\r\n                            <tr v-for=\"(item,index) in items\" :key=\"item.id_layanan\">\r\n                                <td>{{ index + 1 }}</td>\r\n                                <td>{{ item.nama }}</td>\r\n                                <td>{{ item.harga}}</td>\r\n                            </tr>\r\n                        </tbody>\r\n                    </template>\r\n                </v-data-table>\r\n                <div v-if=\"showable\">\r\n                    <v-data-table :headers=\"headers_LOG\" :items=\"servicesLog\" :search=\"keyword\" :loading=\"load\">\r\n                    <template v-slot:body=\"{ items }\">\r\n                        <tbody>\r\n                            <tr v-for=\"(item,index) in items\" :key=\"item.id_layanan\">\r\n                                <td>{{ index + 1 }}</td>\r\n                                <td>{{ item.nama }}</td>\r\n                                <td>{{ item.created_at}}</td>\r\n                                <td>{{ item.update_at}}</td>\r\n                                <td>{{ item.delete_at}}</td>\r\n                                <td>{{ item.aktor }}</td>\r\n                            </tr>\r\n                        </tbody>\r\n                    </template>\r\n                </v-data-table>\r\n                </div>\r\n                <div class=\"nb\">NB : Harga belum termasuk jenis dan ukuran hewan peliharan</div>\r\n            </v-container>\r\n        </v-card>\r\n        <v-snackbar v-model=\"snackbar\" :color=\"color\" :multi-line=\"true\" :timeout=\"3000\">\r\n            {{ text }}\r\n            <v-btn dark text @click=\"snackbar = false\">\r\n                Close\r\n            </v-btn>\r\n        </v-snackbar>\r\n    </v-container>\r\n</template>\r\n<script> \r\nimport { log } from 'util'\r\nexport default {    \r\n    data () {       \r\n        return {      \r\n            showable: false,   \r\n            dialog: false,         \r\n            keyword: '',         \r\n            headers: [             \r\n                {               \r\n                    text: 'No',               \r\n                    value: 'no',             \r\n                },             \r\n                {               \r\n                    text: 'Jenis Layanan',               \r\n                    value: 'nama'             \r\n                },             \r\n                {               \r\n                    text: 'Harga',               \r\n                    value: 'harga'             \r\n                },       \r\n            ],         \r\n            services: [],         \r\n            servicesLog: [],         \r\n            snackbar: false,          \r\n            color: null,         \r\n            text: '',          \r\n            load: false, \r\n            form: {            \r\n                nama : '',           \r\n                harga : ''\r\n            },         \r\n            service : new FormData,         \r\n            typeInput: 'new',        \r\n            errors : '',         \r\n            updatedId : '',       \r\n        }     \r\n    },     \r\n    methods:{         \r\n        getData(){             \r\n            var uri = this.$apiUrl + '/layanan'             \r\n            this.$http.get(uri).then(response =>{                 \r\n                this.services=response.data.data             \r\n            })               \r\n        },  \r\n        getDataLog(){             \r\n            var uri = this.$apiUrl + '/layanan/log'             \r\n            this.$http.get(uri).then(response =>{                 \r\n                this.servicesLog=response.data.data             \r\n            })               \r\n        },                \r\n        \r\n        \r\n        setForm(){             \r\n            if (this.typeInput === 'new') {               \r\n                this.sendData()             \r\n            } else {\r\n                this.updateData()             \r\n            }         \r\n        },         \r\n        resetForm(){             \r\n            this.form = {               \r\n                layanan : '',               \r\n                harga : ''\r\n            }         \r\n        }     \r\n    },     \r\n    mounted(){         \r\n        this.getData();     \r\n        this.getDataLog()\r\n        }, \r\n    } \r\n</script> \r\n<style lang=\"scss\" scoped>\r\n.v-btn {\r\n  color : white;\r\n}\r\n.nb{\r\n    padding-top: 3%;\r\n    text-align: right;\r\n}\r\n</style>"]}]}