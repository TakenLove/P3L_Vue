n            <v-layout row wrap style=\"margin:10px\">        \r\n                <v-flex xs6 class=\"text-right\">               \r\n                    <v-text-field                 \r\n                    v-model=\"keyword\"                 \r\n                    append-icon=\"mdi-search-web\"                 \r\n                    label=\"Search\"                 \r\n                    hide-details               \r\n                    ></v-text-field>           \r\n                </v-flex>         \r\n            </v-layout> \r\n \r\n            <v-data-table             \r\n            :headers=\"headers\"             \r\n            :items=\"request\"             \r\n            :search=\"keyword\"             \r\n            :loading=\"load\"           \r\n            >             \r\n            <template v-slot:body=\"{ items }\">               \r\n                <tbody>                 \r\n                    <tr v-for=\"(item,index) in items\" :key=\"item.id\">                   \r\n                        <td>{{ index + 1 }}</td>                   \r\n                        <td>{{ item.email }}</td>                   \r\n                        <td>{{ item.tittlereq}}</td>                   \r\n                        <td>{{ item.judulreq }}</td>                   \r\n                        <td>{{ item.authorreq }}</td>\r\n                        <td>{{ item.publisherreq }}</td>           \r\n                    </tr>               \r\n                </tbody>             \r\n            </template>           \r\n        </v-data-table>       \r\n    </v-container>     \r\n    </v-card>    \r\n    <v-snackbar       \r\n    v-model=\"snackbar\" \r\n    :color=\"color\"       \r\n    :multi-line=\"true\"       \r\n    :timeout=\"3000\"     \r\n    >       \r\n    {{ text }}       \r\n        <v-btn           \r\n            text         \r\n            @click=\"snackbar = false\"       \r\n        >         \r\n            Close       \r\n        </v-btn>     \r\n    </v-snackbar>   \r\n</v-container> \r\n</template> \r\n<script> \r\nimport { log } from 'util'\r\nexport default {    \r\n    data () {       \r\n        return {         \r\n            dialog: false,         \r\n            keyword: '',         \r\n            headers: [             \r\n                {               \r\n                    text: 'No',               \r\n                    value: 'no',             \r\n                },             \r\n                {               \r\n                    text: 'User',               \r\n                    value: 'email'             \r\n                },             \r\n                {               \r\n                    text: 'Book Tittle',               \r\n                    value: 'tittlereq'             \r\n                },             \r\n                {               \r\n                    text: 'Author',               \r\n                    value: 'authorreq'             \r\n                },\r\n                {               \r\n                    text: 'Publisher',               \r\n                    value: 'publisherreq'             \r\n                },         \r\n            ],         \r\n            requests: [],         \r\n            snackbar: false,          \r\n            color: null,         \r\n            text: '',          \r\n            load: false, \r\n            form: {            \r\n                email : '',           \r\n                judulreq : '',           \r\n                authorreq : '',\r\n                publisherreq : ''         \r\n            },         \r\n            request : new FormData,         \r\n            typeInput: 'new',        \r\n            errors : '',         \r\n            updatedId : '',       \r\n        }     \r\n    },     \r\n    methods:{         \r\n        getData(){             \r\n            var uri = this.$apiUrl + '/requestAdmin'             \r\n            this.$http.get(uri).then(response =>{                 \r\n                this.requests=response.data.message             \r\n            })               \r\n        },         \r\n        setForm(){             \r\n            if (this.typeInput === 'new') {               \r\n                this.sendData()             \r\n            } else {               \r\n                console.log(\"dddd\")\r\n                this.updateData()             \r\n            }         \r\n        },         \r\n        resetForm(){             \r\n            this.form = {               \r\n                email : '',           \r\n                judulreq : '',           \r\n                authorreq : '',\r\n                publisherreq : ''         \r\n            }         \r\n        }     \r\n    },     \r\n    mounted(){         \r\n        this.getData();     \r\n        }, \r\n    } \r\n</script> "]}]}