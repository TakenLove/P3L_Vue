\n                <v-icon>mdi-arrow-up</v-icon>\r\n              </v-btn>\r\n              <v-btn\r\n                large\r\n                depressed\r\n                color=\"#B7472A\"\r\n                :value=\"true\"\r\n              >\r\n                <v-icon>mdi-arrow-down</v-icon>\r\n              </v-btn>\r\n            </v-btn-toggle>\r\n          </template>\r\n        </v-toolbar>\r\n      </template>\r\n\r\n      <template v-slot:default=\"props\">\r\n        <v-row>\r\n          <v-col\r\n            v-for=\"item in props.items\"\r\n            :key=\"item.nama\"\r\n            cols=\"12\"\r\n            sm=\"6\"\r\n            md=\"4\"\r\n            lg=\"3\"\r\n          >\r\n            <v-card>\r\n              <v-card-title class=\"subheading font-weight-bold\">{{ item.nama }}</v-card-title>\r\n\r\n              <v-divider></v-divider>\r\n\r\n              <v-list dense>\r\n                <v-img class=\"white--text align-end\" height=\"200px\" v-bind:src=\"'http://localhost:8081/API_REST/upload/produk/' + item.foto\"></v-img>\r\n                <v-list-item\r\n                  v-for=\"(key, index) in filteredKeys\"\r\n                  :key=\"index\"\r\n                >\r\n                  <v-list-item-content :class=\"{ 'blue--text': sortBy === key }\">{{ key }}:</v-list-item-content>\r\n                  <v-list-item-content class=\"align-end\" :class=\"{ 'blue--text': sortBy === key }\">{{ item[key.toLowerCase()] }}</v-list-item-content>\r\n                </v-list-item>\r\n              </v-list>\r\n            </v-card>\r\n          </v-col>\r\n        </v-row>\r\n      </template>\r\n\r\n      <template v-slot:footer>\r\n        <v-row class=\"mt-2\" align=\"center\" justify=\"center\">\r\n          <span class=\"grey--text\">Items per page</span>\r\n          <v-menu offset-y>\r\n            <template v-slot:activator=\"{ on }\">\r\n              <v-btn\r\n                dark\r\n                text\r\n                color=\"#B7472A\"\r\n                class=\"ml-2\"\r\n                v-on=\"on\"\r\n              >\r\n                {{ itemsPerPage }}\r\n                <v-icon>mdi-chevron-down</v-icon>\r\n              </v-btn>\r\n            </template>\r\n            <v-list>\r\n              <v-list-item\r\n                v-for=\"(number, index) in itemsPerPageArray\"\r\n                :key=\"index\"\r\n                @click=\"updateItemsPerPage(number)\"\r\n              >\r\n                <v-list-item-title>{{ number }}</v-list-item-title>\r\n              </v-list-item>\r\n            </v-list>\r\n          </v-menu>\r\n\r\n          <v-spacer></v-spacer>\r\n\r\n          <span\r\n            class=\"mr-4\r\n            grey--text\"\r\n          >\r\n            Page {{ page }} of {{ numberOfPages }}\r\n          </span>\r\n          <v-btn\r\n            fab\r\n            dark\r\n            color=\"#B7472A\"\r\n            class=\"mr-1\"\r\n            @click=\"formerPage\"\r\n          >\r\n            <v-icon>mdi-chevron-left</v-icon>\r\n          </v-btn>\r\n          <v-btn\r\n            fab\r\n            dark\r\n            color=\"#B7472A\"\r\n            class=\"ml-1\"\r\n            @click=\"nextPage\"\r\n          >\r\n            <v-icon>mdi-chevron-right</v-icon>\r\n          </v-btn>\r\n        </v-row>\r\n      </template>\r\n    </v-data-iterator>\r\n  </v-container>\r\n</template>\r\n\r\n<script>\r\n  export default {\r\n    data () {\r\n      return {\r\n        itemsPerPageArray: [4, 8, 12],\r\n        search: '',\r\n        filter: {},\r\n        sortDesc: false,\r\n        page: 1,\r\n        itemsPerPage: 4,\r\n        sortBy: 'nama',\r\n        keys: [\r\n          'Unit',\r\n          'Stok',\r\n          'Harga',\r\n        ],\r\n        items: [],\r\n      }\r\n    },\r\n    computed: {\r\n      numberOfPages () {\r\n        return Math.ceil(this.items.length / this.itemsPerPage)\r\n      },\r\n      filteredKeys () {\r\n        return this.keys.filter(key => key !== `Nama`)\r\n      }\r\n      \r\n    },\r\n    methods: {\r\n    getData() {\r\n            var uri = this.$apiUrl + '/produk'\r\n            this.$http.get(uri).then(response => {\r\n                this.items = response.data.data\r\n            })\r\n        },\r\n      nextPage () {\r\n        if (this.page + 1 <= this.numberOfPages) this.page += 1\r\n      },\r\n      formerPage () {\r\n        if (this.page - 1 >= 1) this.page -= 1\r\n      },\r\n      updateItemsPerPage (number) {\r\n        this.itemsPerPage = number\r\n      },\r\n    },\r\n    mounted() {\r\n        this.getData();\r\n    },\r\n  }\r\n</script>"],"sourceRoot":"src/components/homePageContents"}]}