ACA,QAAA,IAAA,EAAA,cADA;AAEA,QAAA,KAAA,EAAA;AAFA,OAbA,EAiBA;AACA,QAAA,IAAA,EAAA,YADA;AAEA,QAAA,KAAA,EAAA;AAFA,OAjBA,EAqBA;AACA,QAAA,IAAA,EAAA,MADA;AAEA,QAAA,KAAA,EAAA;AAFA,OArBA,CAHA;AA6BA,MAAA,OAAA,EAAA,EA7BA;AA8BA,MAAA,QAAA,EAAA,KA9BA;AA+BA,MAAA,KAAA,EAAA,IA/BA;AAgCA,MAAA,IAAA,EAAA,EAhCA;AAiCA,MAAA,IAAA,EAAA,KAjCA;AAkCA,MAAA,IAAA,EAAA;AACA,QAAA,MAAA,EAAA;AADA,OAlCA;AAqCA,MAAA,MAAA,EAAA,IAAA,QAAA,EArCA;AAsCA,MAAA,SAAA,EAAA,KAtCA;AAuCA,MAAA,MAAA,EAAA,EAvCA;AAwCA,MAAA,SAAA,EAAA;AAxCA,KAAA;AA0CA,GA5CA;AA6CA,EAAA,OAAA,EAAA;AACA,IAAA,OADA,qBACA;AAAA;;AACA,UAAA,GAAA,GAAA,KAAA,OAAA,GAAA,SAAA;AACA,WAAA,KAAA,CAAA,GAAA,CAAA,GAAA,EAAA,IAAA,CAAA,UAAA,QAAA,EAAA;AACA,QAAA,KAAA,CAAA,OAAA,GAAA,QAAA,CAAA,IAAA,CAAA,OAAA;AACA,OAFA;AAGA,KANA;AAOA,IAAA,UAPA,wBAOA;AAAA;;AACA,WAAA,MAAA,CAAA,MAAA,CAAA,QAAA,EAAA,KAAA,IAAA,CAAA,MAAA;AACA,UAAA,GAAA,GAAA,KAAA,OAAA,GAAA,UAAA,GAAA,KAAA,SAAA;AACA,WAAA,IAAA,GAAA,IAAA;AACA,WAAA,KAAA,CAAA,IAAA,CAAA,GAAA,EAAA,KAAA,MAAA,EAAA,IAAA,CAAA,UAAA,QAAA,EAAA;AACA,QAAA,MAAA,CAAA,QAAA,GAAA,IAAA,CADA,CACA;;AACA,QAAA,MAAA,CAAA,KAAA,GAAA,OAAA,CAFA,CAEA;;AACA,QAAA,MAAA,CAAA,IAAA,GAAA,QAAA,CAAA,IAAA,CAAA,OAAA,CAHA,CAGA;;AACA,QAAA,MAAA,CAAA,IAAA,GAAA,KAAA;AACA,QAAA,MAAA,CAAA,MAAA,GAAA,KAAA;;AACA,QAAA,MAAA,CAAA,OAAA,GANA,CAMA;;;AACA,QAAA,MAAA,CAAA,SAAA;;AACA,QAAA,MAAA,CAAA,SAAA,GAAA,KAAA;AACA,OATA,EASA,KATA,CASA,UAAA,KAAA,EAAA;AACA,QAAA,MAAA,CAAA,MAAA,GAAA,KAAA;AACA,QAAA,MAAA,CAAA,QAAA,GAAA,IAAA;AACA,QAAA,MAAA,CAAA,IAAA,GAAA,WAAA;AACA,QAAA,MAAA,CAAA,KAAA,GAAA,KAAA;AACA,QAAA,MAAA,CAAA,IAAA,GAAA,KAAA;AACA,QAAA,MAAA,CAAA,SAAA,GAAA,KAAA;AACA,OAhBA;AAiBA,KA5BA;AA6BA,IAAA,WA7BA,uBA6BA,IA7BA,EA6BA;AACA,WAAA,SAAA,GAAA,MAAA;AACA,WAAA,MAAA,GAAA,IAAA;AACA,WAAA,IAAA,CAAA,MAAA,GAAA,IAAA,CAAA,MAAA;AACA,WAAA,SAAA,GAAA,IAAA,CAAA,EAAA;AACA,KAlCA;AAmCA,IAAA,OAnCA,qBAmCA;AACA,UAAA,KAAA,SAAA,KAAA,KAAA,EAAA;AACA,aAAA,QAAA;AACA,OAFA,MAEA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,MAAA;AACA,aAAA,UAAA;AACA;AACA,KA1CA;AA2CA,IAAA,SA3CA,uBA2CA;AACA,WAAA,IAAA,GAAA;AACA,QAAA,MAAA,EAAA;AADA,OAAA;AAGA;AA/CA,GA7CA;AA8FA,EAAA,OA9FA,qBA8FA;AACA,SAAA,OAAA;AACA;AAhGA,CAAA","sourcesContent":["<template>   \r\n    <v-container>     \r\n        <v-card>       \r\n            <v-container grid-list-md mb-0>\r\n            <h2 class=\"text-md-center\">User Borrow List</h2>         \r\n            <v-layout row wrap style=\"margin:10px\">                  \r\n                <v-flex xs6 class=\"text-right\">               \r\n                    <v-text-field                 \r\n                    v-model=\"keyword\"                 \r\n                    append-icon=\"mdi-search-web\"                 \r\n                    label=\"Search\"                 \r\n                    hide-details               \r\n                    ></v-text-field>           \r\n                </v-flex>         \r\n            </v-layout> \r\n \r\n            <v-data-table             \r\n            :headers=\"headers\"             \r\n            :items=\"borrows\"             \r\n            :search=\"keyword\"             \r\n            :loading=\"load\"           \r\n            >             \r\n            <template v-slot:body=\"{ items }\">               \r\n                <tbody>                 \r\n                    <tr v-for=\"(item,index) in items\" :key=\"item.id\">                   \r\n                        <td>{{ index + 1 }}</td>                   \r\n                        <td>{{ item.name }}</td>                     \r\n                        <td>{{ item.phoneNumber }}</td>                   \r\n                        <td>{{ item.tittlebor }}</td>\r\n                        <td>{{ item.authorbor }}</td>\r\n                        <td>{{ item.statusbor }}</td>\r\n                        <td class=\"text-center\">                     \r\n                            <v-btn                        \r\n                            icon                       \r\n                            color=\"indigo\"                       \r\n                            light                       \r\n                            @click=\"editHandler(item)\"                     \r\n                            >                       \r\n                            <v-icon>mdi-pencil</v-icon>                     \r\n                            </v-btn>                     \r\n                        </td>                 \r\n                    </tr>               \r\n                </tbody>             \r\n            </template>           \r\n        </v-data-table>       \r\n    </v-container>     \r\n    </v-card>     \r\n    <v-dialog v-model=\"dialog\" persistent max-width=\"600px\">       \r\n        <v-card>         \r\n            <v-card-title>           \r\n                <span class=\"headline\">Change Book's Status</span>         \r\n                </v-card-title>         \r\n                <v-card-text>           \r\n                    <v-container>             \r\n                        <v-row>               \r\n                            <v-col cols=\"12\">                 \r\n                                <v-text-field label=\"Status*\" v-model=\"form.status\" required></v-text-field>               \r\n                                </v-col>                           \r\n                            </v-row>           \r\n                                </v-container>           \r\n                                <small>*indicates required field</small>         \r\n                            </v-card-text>         \r\n                            <v-card-actions>           \r\n                                <v-spacer></v-spacer>           \r\n                                <v-btn color=\"blue darken-1\" text @click=\"dialog = false\">Close</v-btn>           \r\n                                <v-btn color=\"blue darken-1\" text @click=\"setForm()\">Save</v-btn>         \r\n                                </v-card-actions>       \r\n                            </v-card>     \r\n                        </v-dialog>     \r\n                    <v-snackbar       \r\n                    v-model=\"snackbar\" \r\n                    :color=\"color\"       \r\n                    :multi-line=\"true\"       \r\n                    :timeout=\"3000\"     \r\n                    >       \r\n                    {{ text }}       \r\n                    <v-btn         \r\n                     dark         \r\n                     text         \r\n                     @click=\"snackbar = false\"       \r\n                    >         \r\n                        Close       \r\n                    </v-btn>     \r\n                </v-snackbar>   \r\n            </v-container> \r\n        </template> \r\n<script> \r\nimport { log } from 'util'\r\nexport default {    \r\n    data () {       \r\n        return {         \r\n            dialog: false,         \r\n            keyword: '',         \r\n            headers: [             \r\n                {               \r\n                    text: 'No',               \r\n                    value: 'no',             \r\n                },             \r\n                {               \r\n                    text: 'Name',               \r\n                    value: 'name'             \r\n                },             \r\n                {               \r\n                    text: 'Address',               \r\n                    value: 'address'             \r\n                },             \r\n                {               \r\n                    text: 'Phone Number',               \r\n                    value: 'phoneNumber'             \r\n                },\r\n                {\r\n                    text: 'Created At',\r\n                    value: 'created_at'\r\n                },             \r\n                {               \r\n                    text: 'Aksi',               \r\n                    value: null             \r\n                },         \r\n            ],         \r\n            borrows: [],         \r\n            snackbar: false,          \r\n            color: null,         \r\n            text: '',          \r\n            load: false, \r\n            form: {            \r\n                status : ''         \r\n            },         \r\n            borrow : new FormData,         \r\n            typeInput: 'new',        \r\n            errors : '',         \r\n            updatedId : '',       \r\n        }     \r\n    },     \r\n    methods:{         \r\n        getData(){             \r\n            var uri = this.$apiUrl + '/borrow'             \r\n            this.$http.get(uri).then(response =>{                 \r\n                this.borrows=response.data.message             \r\n            })               \r\n        },         \r\n        updateData(){             \r\n            this.borrow.append('status', this.form.status);        \r\n            var uri = this.$apiUrl + '/borrow/' + this.updatedId;             \r\n            this.load = true             \r\n            this.$http.post(uri,this.borrow).then(response =>{ \r\n                this.snackbar = true; //mengaktifkan snackbar               \r\n                this.color = 'green'; //memberi warna snackbar               \r\n                this.text = response.data.message; //memasukkan pesan ke snackbar               \r\n                this.load = false;               \r\n                this.dialog = false               \r\n                this.getData(); //mengambil data user               \r\n                this.resetForm();               \r\n                this.typeInput = 'new';           \r\n            }).catch(error =>{               \r\n                this.errors = error               \r\n                this.snackbar = true;               \r\n                this.text = 'Try Again';               \r\n                this.color = 'red';               \r\n                this.load = false;               \r\n                this.typeInput = 'new';           \r\n            })         \r\n        },         \r\n        editHandler(item){           \r\n            this.typeInput = 'edit';           \r\n            this.dialog = true;           \r\n            this.form.status = item.status;                    \r\n            this.updatedId = item.id         \r\n        },\r\n        setForm(){             \r\n            if (this.typeInput === 'new') {               \r\n                this.sendData()             \r\n            } else {               \r\n                console.log(\"dddd\")\r\n                this.updateData()             \r\n            }         \r\n        },         \r\n        resetForm(){             \r\n            this.form = {               \r\n                status : ''\r\n            }         \r\n        }     \r\n    },     \r\n    mounted(){         \r\n        this.getData();     \r\n        }, \r\n    } \r\n</script> "],"sourceRoot":"src/components/dashboardAdminContents"}]}