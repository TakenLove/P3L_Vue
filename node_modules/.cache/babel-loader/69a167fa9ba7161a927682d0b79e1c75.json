ms=\"products\" :search=\"keyword\" :loading=\"load\" v-if=\"!showable\">\r\n                    <template v-slot:body=\"{ items }\">\r\n                        <tbody>\r\n                            <tr v-for=\"(item,index) in items\" :key=\"item.id_produk\">\r\n                                <td>{{ index + 1 }}</td>\r\n                                <td>{{ item.nama }}</td>\r\n                                <td>{{ item.unit }}</td>\r\n                                <td>\r\n                                    <v-chip :color=\"getColor(item)\" dark>{{ item.stok }}</v-chip>\r\n                                </td>\r\n                                <td>{{ item.min_stok }}</td>\r\n                                <td>{{ item.harga }}</td>\r\n                                <v-img class=\"white--text align-end\" height=\"70px\" width=\"50px\"\r\n                                    v-bind:src=\"'http://localhost:8081/API_REST/upload/produk/' + item.foto\">\r\n                                </v-img>\r\n                                <td class=\"text-center\">\r\n                                    <v-btn icon color=\"indigo\" light @click=\"editHandler(item)\">\r\n                                        <v-icon>mdi-pencil</v-icon>\r\n                                    </v-btn>\r\n                                    <v-btn icon color=\"error\" light @click=\"deleteData(item.id_produk)\">\r\n                                        <v-icon>mdi-delete</v-icon>\r\n                                    </v-btn>\r\n                                </td>\r\n                            </tr>\r\n                        </tbody>\r\n                    </template>\r\n                </v-data-table>\r\n                <div v-if=\"showable\">\r\n                    <v-data-table :headers=\"headers_LOG\" :items=\"productsLog\" :search=\"keyword\" :loading=\"load\">\r\n                    <template v-slot:body=\"{ items }\">\r\n                        <tbody>\r\n                            <tr v-for=\"(item,index) in items\" :key=\"item.id_produk\">\r\n                                <td>{{ index + 1 }}</td>\r\n                                <td>{{ item.nama }}</td>\r\n                                <td>\r\n                                    <v-img class=\"white--text align-end\" height=\"70px\" width=\"50px\"\r\n                                        v-bind:src=\"image + item.foto\">\r\n                                    </v-img>\r\n                                </td>\r\n                                <td>{{ item.created_at }}</td>\r\n                                <td>{{ item.update_at }}</td>\r\n                                <td>{{ item.delete_at }}</td>\r\n                                <td>{{ item.aktor }}</td>\r\n                            </tr>\r\n                        </tbody>\r\n                    </template>\r\n                </v-data-table>\r\n                </div>\r\n            </v-container>\r\n        </v-card>\r\n        <v-dialog v-model=\"dialog\" persistent max-width=\"600px\">\r\n            <v-card>\r\n                <v-card-title>\r\n                    <span class=\"headline\">Produk Data</span>\r\n                </v-card-title>\r\n                <v-card-text>\r\n                    <v-container>\r\n                        <v-row>\r\n                            <v-col cols=\"12\">\r\n                                <v-text-field label=\"Nama Produk*\" v-model=\"form.nama\" required></v-text-field>\r\n                            </v-col>\r\n                            <v-col cols=\"12\">\r\n                                <v-text-field label=\"Keterangan*\" v-model=\"form.unit\" required></v-text-field>\r\n                            </v-col>\r\n                            <v-col cols=\"12\">\r\n                                <v-text-field label=\"Stok*\" v-model=\"form.stok\" required></v-text-field>\r\n                            </v-col>\r\n                            <v-col cols=\"12\">\r\n                                <v-text-field label=\"Minimal Stok*\" v-model=\"form.min_stok\" required></v-text-field>\r\n                            </v-col>\r\n                            <v-col cols=\"12\">\r\n                                <v-text-field label=\"Harga*\" v-model=\"form.harga\" required></v-text-field>\r\n                            </v-col>\r\n                            <v-col cols=\"12\">\r\n                                <v-file-input v-model=\"form.foto\" label=\"Pilih Gambar Produk*\"\r\n                                    accept=\"image/png, image/jpeg, image/bmp, image/jpg\" @change=\"onFileChange\"\r\n                                    prepend-icon=\"mdi-camera\"></v-file-input>\r\n                            </v-col>\r\n                        </v-row>\r\n                        <small>*indicates required field</small>\r\n                    </v-container>\r\n                    <v-card-actions>\r\n                        <v-spacer></v-spacer>\r\n                        <v-btn color=\"blue darken-1\" text @click=\"dialog = false\">Close</v-btn>\r\n                        <v-btn color=\"blue darken-1\" text @click=\"setForm()\">Save</v-btn>\r\n                    </v-card-actions>\r\n                </v-card-text>\r\n            </v-card>\r\n        </v-dialog>\r\n        <v-snackbar v-model=\"snackbar\" :color=\"color\" :multi-line=\"true\" :timeout=\"3000\">\r\n            {{ text }}\r\n            <v-btn dark text @click=\"snackbar = false\">\r\n                Close\r\n            </v-btn>\r\n        </v-snackbar>\r\n    </v-container>\r\n</template>\r\n<script>\r\n\r\nimport { log } from 'util'\r\nexport default {    \r\n    data () {       \r\n        return {       \r\n            image: 'http://localhost:8081/API_REST/upload/produk/',   \r\n            seen: false,\r\n            showable: false,  \r\n            dialog: false,         \r\n            keyword: '',         \r\n            headers: [             \r\n                {               \r\n                    text: 'No',               \r\n                    value: 'no',             \r\n                },             \r\n                {               \r\n                    text: 'Nama Produk',               \r\n                    value: 'nama'             \r\n                },             \r\n                {               \r\n                    text: 'Keterangan',               \r\n                    value: 'unit'             \r\n                },             \r\n                {               \r\n                    text: 'Stok',               \r\n                    value: 'stok'             \r\n                },             \r\n                {               \r\n                    text: 'Minimal Stok',               \r\n                    value: 'min_stok'             \r\n                }, \r\n                {               \r\n                    text: 'Harga',               \r\n                    value: 'harga'             \r\n                },\r\n                {\r\n                    text: 'Foto',\r\n                    value: 'foto'\r\n                },       \r\n                {               \r\n                    text: 'Aksi',               \r\n                    value: null             \r\n                },         \r\n            ],headers_LOG: [             \r\n                {               \r\n                    text: 'No',               \r\n                    value: 'no',             \r\n                },             \r\n                {               \r\n                    text: 'Nama Produk',               \r\n                    value: 'nama'             \r\n                },             \r\n                {\r\n                    text: 'Foto',\r\n                    value: 'foto'\r\n                },\r\n                {               \r\n                    text: 'Dibuat Tanggal',               \r\n                    value: 'created_at'             \r\n                },\r\n                {               \r\n                    text: 'Diubah Tanggal',               \r\n                    value: 'update_at'             \r\n                },\r\n                {               \r\n                    text: 'Dihapus Tanggal',               \r\n                    value: 'delete_at'             \r\n                },\r\n                {               \r\n                    text: 'Aktor',               \r\n                    value: 'aktor'             \r\n                },         \r\n            ],                    \r\n            products: [],\r\n            productsLog: [],         \r\n            snackbar: false,          \r\n            color: null,         \r\n            text: '',          \r\n            load: false, \r\n            form: {            \r\n                nama : '',           \r\n                unit : '',\r\n                stok : '',           \r\n                min_stok : '',\r\n                harga : '',\r\n                foto : null,\r\n            },         \r\n            product : new FormData,         \r\n            typeInput: 'new',        \r\n            errors : '',         \r\n            updatedId : '', \r\n            imageUrl : null,          \r\n        }     \r\n    },     \r\n    methods:{         \r\n        getData(){             \r\n            var uri = this.$apiUrl + '/produk'             \r\n            this.$http.get(uri).then(response =>{                 \r\n                this.products=response.data.data             \r\n            })               \r\n        },  \r\n        getDataLog(){             \r\n            var uri = this.$apiUrl + '/produk/log'             \r\n            this.$http.get(uri).then(response =>{                 \r\n                this.productsLog=response.data.data             \r\n            })               \r\n        },  \r\n        sendData(){             \r\n            this.product.append('nama', this.form.nama);             \r\n            this