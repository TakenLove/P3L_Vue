o-wrap justify-space-between\">\r\n              <div>\r\n                <v-card-title\r\n                  class=\"headline\"\r\n                  v-text=\"item.title\"\r\n                ></v-card-title>\r\n\r\n                <v-card-subtitle clss=\"fonr-weight-bold\" v-text=\"item.artist\"></v-card-subtitle>\r\n                <v-card-actions>\r\n                  <v-btn text @click=\"sign(item)\">Show More</v-btn>\r\n                </v-card-actions>\r\n              </div>\r\n              <v-avatar\r\n                class=\"ma-3\"\r\n                size=\"125\"\r\n                tile\r\n              >\r\n                <v-img :src=\"item.src\"></v-img>\r\n              </v-avatar>\r\n            </div>\r\n          </v-card>\r\n        </v-col>\r\n      </v-row>\r\n    </v-container>\r\n  </v-card>\r\n</template>\r\n<script> \r\nimport { log } from 'util'\r\nexport default {    \r\n    data () {       \r\n        return {         \r\n            items: [\r\n            {\r\n              color: '#D6B066',\r\n              src: 'https://pngimg.com/uploads/box/box_PNG132.png',\r\n              title: 'Add New Stock Product',\r\n              artist: 'Check your product so you can restok your product if your stock start to be empty.',\r\n              // button: this.sign(),\r\n            },\r\n            {\r\n              color: '#D29C76',\r\n              src: 'https://www.storage.ie/wp-content/uploads/2017/03/document_storage-1.jpg',\r\n              title: 'Show Me My Stock',\r\n              artist: 'Show me All Data in My Stock',\r\n              // button: this.$router.replace({ path : '/dashboardAdmin' }),\r\n            },\r\n            {\r\n              color: '#3C3D3F',\r\n              src: 'https://ak.picdn.net/shutterstock/videos/15071320/thumb/4.jpg',\r\n              title: 'My Partner',\r\n              artist: 'Check on my partnet for my pet shop',\r\n              // button: this.$router.replace({ path : '/dashboardAdmin' }),\r\n            },\r\n          ],\r\n          order : new FormData,         \r\n        }     \r\n    },     \r\n    methods:{         \r\n        sign(item){\r\n        if(item.title === 'Add New Stock Product'){\r\n          this.sendData(),\r\n          this.$router.replace({ path : '/dashboardAdmin/AddStock' })\r\n        }else if(item.title === 'Show Me My Stock'){\r\n          this.$router.replace({ path : '/dashboardAdmin/ShowStock' })\r\n        }else{\r\n          this.$router.replace({ path : '/dashboardAdmin/addSupplier' }) \r\n        }\r\n      },\r\n      sendData(){       \r\n            this.order.append('id_pengadaan', 1);\r\n            var uri =this.$apiUrl + '/pengadaan_produk'             \r\n            this.load = true;             \r\n            this.$http.post(uri,this.order).then(response =>{               \r\n                this.snackbar = true; //mengaktifkan snackbar               \r\n                this.color = 'green'; //memberi warna snackbar               \r\n                this.text = response.data.message; //memasukkan pesan ke snackbar               \r\n                this.load = false;                 \r\n                this.getData(); //mengambil data user               \r\n                this.resetForm(); //mengambil data ukuran                       \r\n            }).catch(error =>{               \r\n                this.errors = error               \r\n                this.snackbar = true;               \r\n                this.text = 'Try Again';               \r\n                this.color = 'red';               \r\n                this.load = false;           \r\n            })\r\n        },     \r\n    },      \r\n    } \r\n</script> \r\n\r\n\r\n<style lang=\"scss\" scoped>\r\n.margin{\r\n  padding-top: 4%;\r\n  padding-bottom: 6%;\r\n}\r\n</style>"],"sourceRoot":"src/components/dashboardAdminContents"}]}