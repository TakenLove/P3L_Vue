ow>\r\n            <v-col v-for=\"item in props.items\" :key=\"item.book_id\" cols=\"12\" sm=\"6\" md=\"4\" lg=\"3\">\r\n                \r\n                <v-dialog v-model=\"dialog\" persistent max-width=\"600px\">\r\n                    <v-card>\r\n                        <v-card-title>\r\n                            <span class=\"headline\">Review</span>\r\n                        </v-card-title>\r\n                        <v-card-text>\r\n                            <v-container>\r\n                                <v-row>\r\n                                    <v-col cols=\"12\">\r\n                                        <v-text-field label=\"Review*\" v-model=\"form.review\" required></v-text-field>\r\n                                    </v-col>\r\n                                    <v-col cols=\"12\">\r\n                                        <v-text-field label=\"Rating ( 1 - 5 )*\" v-model=\"form.rating\" required></v-text-field>\r\n                                    </v-col>\r\n                                </v-row>\r\n                            </v-container>\r\n                            <small>*indicates required field</small>\r\n                        </v-card-text>\r\n                        <v-card-actions>\r\n                            <v-spacer></v-spacer>\r\n                            <v-btn color=\"blue darken-1\" text @click=\"dialog = false\">Close</v-btn>\r\n                            <v-btn color=\"blue darken-1\" text @click=\"sendData()\">Save</v-btn>\r\n                        </v-card-actions>\r\n                    </v-card>\r\n                </v-dialog>\r\n\r\n                <v-card>\r\n                    <v-img class=\"white--text align-end\" height=\"200px\" :src=\"require(`C:/xampp/htdocs/PAW_Aplikasi_CI_C_Kel7/upload/product/${item.book_image}`)\"></v-img>\r\n\r\n                    <v-divider></v-divider>\r\n\r\n                    <v-list dense>\r\n                        <v-list-item>\r\n                            <v-list-item-content>Book Title</v-list-item-content>\r\n                            <v-list-item-content class=\"align-end\">: {{ item.title }}</v-list-item-content>\r\n                        </v-list-item>\r\n\r\n                        <v-list-item>\r\n                            <v-list-item-content>Authors</v-list-item-content>\r\n                            <v-list-item-content class=\"align-end\">: {{ item.authors }}</v-list-item-content>\r\n                        </v-list-item>\r\n\r\n                        <v-list-item>\r\n                            <v-list-item-content>Publisher</v-list-item-content>\r\n                            <v-list-item-content class=\"align-end\">: {{ item.publisher }}</v-list-item-content>\r\n                        </v-list-item>\r\n\r\n                        <v-list-item>\r\n                            <v-list-item-content>Category</v-list-item-content>\r\n                            <v-list-item-content class=\"align-end\">: {{ item.category }}</v-list-item-content>\r\n                        </v-list-item>\r\n                        <v-list-item>\r\n                            <v-list-item-content>Status</v-list-item-content>\r\n                            <v-list-item-content class=\"align-end\">: {{ item.status }}</v-list-item-content>\r\n                        </v-list-item>\r\n                    </v-list>\r\n                    \r\n                    <v-divider></v-divider>\r\n\r\n                    <v-card-actions v-slot:activator=\"{ on }\">\r\n                        <v-btn color=\"blue\" dark v-on=\"on\">Give a Review</v-btn>\r\n                    </v-card-actions>\r\n                </v-card>\r\n          </v-col>\r\n        </v-row>\r\n      </template>\r\n    </v-data-iterator>\r\n  </v-container>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n        data() {\r\n            return {\r\n                dialog: false,\r\n                keyword: '',\r\n                books: [],\r\n                snackbar: false,\r\n                color: null,\r\n                text: '',\r\n                load: false,\r\n                form: {\r\n                    review : '',           \r\n                    rating : ''    \r\n                },         \r\n                request: new FormData,\r\n                typeInput: 'new',\r\n                errors: '',\r\n                book_id: '',\r\n            }\r\n        },\r\n        methods: {\r\n            getData() {\r\n                var uri = this.$apiUrl + '/borrowlist'\r\n                this.$http.get(uri).then(response => {\r\n                    this.books = response.data.message\r\n                })\r\n            },\r\n\r\n            review($id){\r\n                this.book_id = $id;\r\n                this.dialog = true;\r\n                this.form.review = item.review;           \r\n                this.form.rating = item.rating;\r\n                console.log(\"as\");\r\n            },\r\n\r\n            sendData(){\r\n                this.request.append('review', this.form.review);      \r\n                this.request.append('rating', this.form.rating);    \r\n                this.request.append('book_id', this.book_id);\r\n                this.request.append('user_id', 1);\r\n                var uri =this.$apiUrl + '/borrowlist'        \r\n                this.load = true             \r\n                this.$http.post(uri,this.request).then(response =>{               \r\n                    this.snackbar = true; //mengaktifkan snackbar               \r\n                    this.color = 'green'; //memberi warna snackbar               \r\n                    this.text = response.data.message; //memasukkan pesan ke snackbar               \r\n                    this.load = false;               \r\n                    this.dialog = false               \r\n                    this.getData(); //mengambil data user               \r\n                    this.resetForm();           \r\n                }).catch(error =>{               \r\n                    this.errors = error               \r\n                    this.snackbar = true;               \r\n                    this.text = 'Try Again';               \r\n                    this.color = 'red';               \r\n                    this.load = false;           \r\n                })         \r\n            },\r\n\r\n            resetForm(){             \r\n                this.form = {               \r\n                    review : '',           \r\n                    rating : ''  \r\n                }         \r\n            },\r\n        },\r\n\r\n        mounted() {\r\n            this.getData();\r\n        },\r\n    }\r\n</script>"],"sourceRoot":"src/components/dashboardUserContents"}]}