A;AAJA,SAAA,CAAA;AAMA,OAPA,CAVA,CAkBA;;;AACA,UAAA,QAAA,GAAA,SAAA,QAAA,GAAA;AACA,YAAA,IAAA,GAAA,EAAA;;AACA,aAAA,IAAA,CAAA,GAAA,CAAA,EAAA,GAAA,GAAA,KAAA,CAAA,QAAA,CAAA,MAAA,EAAA,CAAA,GAAA,GAAA,EAAA,CAAA,EAAA,EAAA;AACA,UAAA,IAAA,CAAA,IAAA,CAAA;AACA,YAAA,EAAA,EAAA,CAAA,GAAA,CADA;AAEA,YAAA,KAAA,EAAA,KAAA,CAAA,QAAA,CAAA,CAAA,EAAA,UAFA;AAGA,YAAA,WAAA,EAAA,KAAA,CAAA,QAAA,CAAA,CAAA,EAAA,WAHA;AAIA,YAAA,gBAAA,EAAA,KAAA,CAAA,QAAA,CAAA,CAAA,EAAA;AAJA,WAAA;AAMA;;AACA,eAAA,IAAA;AACA,OAXA,CAnBA,CA+BA;;;AACA,MAAA,GAAA,CAAA,WAAA,CAAA,EAAA;AACA,MAAA,GAAA,CAAA,YAAA,CAAA,EAAA;AACA,MAAA,GAAA,CAAA,YAAA,CAAA,MAAA;AACA,MAAA,GAAA,CAAA,IAAA,CAAA,yBAAA,EAAA,GAAA,EAAA,GAAA,EAnCA,CAoCA;;AACA,MAAA,GAAA,CAAA,WAAA,CAAA,EAAA;AACA,MAAA,GAAA,CAAA,YAAA,CAAA,EAAA;AACA,MAAA,GAAA,CAAA,YAAA,CAAA,QAAA;AACA,MAAA,GAAA,CAAA,IAAA,CAAA,cAAA,EAAA,EAAA,EAAA,GAAA;AACA,MAAA,GAAA,CAAA,SAAA,CAAA;AACA,QAAA,MAAA,EAAA;AAAA,UAAA,GAAA,EAAA;AAAA,SADA;AAEA,QAAA,IAAA,EAAA,QAAA,EAFA;AAGA,QAAA,IAAA,EAAA,QAAA;AAHA,OAAA,EAzCA,CA8CA;;AACA,MAAA,GAAA,CAAA,WAAA,CAAA,EAAA;AACA,MAAA,GAAA,CAAA,YAAA,CAAA,EAAA;AACA,MAAA,GAAA,CAAA,YAAA,CAAA,QAAA;AACA,UAAA,IAAA,GAAA,IAAA,IAAA,GAAA,cAAA,CAAA,IAAA,EAAA;AAAA,QAAA,KAAA,EAAA,MAAA;AAAA,QAAA,IAAA,EAAA,SAAA;AAAA,QAAA,GAAA,EAAA;AAAA,OAAA,CAAA;AACA,MAAA,GAAA,CAAA,IAAA,CAAA,uBAAA,IAAA,EAAA,EAAA,EAAA,GAAA;AACA,MAAA,GAAA,CAAA,IAAA,CAAA,6BAAA;AACA,WAAA,aAAA;AACA,KA7EA;AA8EA,IAAA,aA9EA,2BA8EA;AACA,WAAA,QAAA,GAAA,IAAA,CADA,CACA;;AACA,WAAA,KAAA,GAAA,OAAA,CAFA,CAEA;;AACA,WAAA,IAAA,GAAA,SAAA,CAHA,CAGA;AACA,KAlFA;AAmFA,IAAA,mBAnFA,+BAmFA,OAnFA,EAmFA;AACA,WAAA,MAAA,GAAA,OAAA;AACA,KArFA;AAsFA,IAAA,OAtFA,qBAsFA;AAAA;;AACA,UAAA,GAAA,GAAA,KAAA,OAAA,GAAA,oCAAA;AACA,WAAA,KAAA,CAAA,GAAA,CAAA,GAAA,EAAA,IAAA,CAAA,UAAA,QAAA,EAAA;AACA,QAAA,MAAA,CAAA,QAAA,GAAA,QAAA,CAAA,IAAA,CAAA,IAAA;AACA,OAFA;AAGA,KA3FA;AA4FA,IAAA,QA5FA,sBA4FA;AAAA;;AACA,WAAA,KAAA,CAAA,MAAA,CAAA,OAAA,EAAA,KAAA,IAAA,CAAA,MAAA;AACA,UAAA,GAAA,GAAA,KAAA,OAAA,GAAA,2BAAA;AACA,WAAA,IAAA,GAAA,IAAA;AACA,WAAA,KAAA,CAAA,IAAA,CAAA,GAAA,EAAA,KAAA,KAAA,EAAA,IAAA,CAAA,UAAA,QAAA,EAAA;AACA,QAAA,MAAA,CAAA,QAAA,GAAA,IAAA,CADA,CACA;;AACA,QAAA,MAAA,CAAA,KAAA,GAAA,OAAA,CAFA,CAEA;;AACA,QAAA,MAAA,CAAA,IAAA,GAAA,QAAA,CAAA,IAAA,CAAA,OAAA,CAHA,CAGA;;AACA,QAAA,MAAA,CAAA,IAAA,GAAA,KAAA;AACA,QAAA,MAAA,CAAA,MAAA,GAAA,KAAA;;AACA,QAAA,MAAA,CAAA,OAAA,GANA,CAMA;;;AACA,QAAA,MAAA,CAAA,SAAA;AACA,OARA,EAQA,KARA,CAQA,UAAA,KAAA,EAAA;AACA,QAAA,MAAA,CAAA,MAAA,GAAA,KAAA;AACA,QAAA,MAAA,CAAA,QAAA,GAAA,IAAA;AACA,QAAA,MAAA,CAAA,IAAA,GAAA,WAAA;AACA,QAAA,MAAA,CAAA,KAAA,GAAA,KAAA;AACA,QAAA,MAAA,CAAA,IAAA,GAAA,KAAA;AACA,OAdA;AAeA;AA/GA,GAjEA;AAkLA,EAAA,OAlLA,qBAkLA;AACA,SAAA,OAAA;AACA,SAAA,uBAAA;AACA;AArLA,CAAA","sourcesContent":["<template>\r\n    <v-container>\r\n        <v-card class=\"content\">\r\n            <v-container grid-list-md px-12 py-12>\r\n                <h2 class=\"text-md-center\">Laporan Produk Terlaris</h2>\r\n                <v-layout row wrap style=\"margin:10px\">\r\n                    <v-flex xs5>\r\n                            <div style=\"width:55%\" class=\"mt-2 ml-0\">\r\n                                <v-select \r\n                                :items=\"nama_tahun\" \r\n                                v-model=\"form.tahun\" \r\n                                label=\"Tahun\" \r\n                                placeholder=\"Pilih Tahun\" \r\n                                autocomplete \r\n                                append-icon='mdi-calendar'>\r\n                                </v-select>\r\n                            </div>\r\n                    </v-flex>\r\n                    <v-flex xs6 class=\"text-right\">\r\n                        <v-tooltip bottom>\r\n                            <template v-slot:activator=\"{ on }\">\r\n                                <v-btn \r\n                                    depressed rounded style=\"text-transform: none !important;\" \r\n                                    color=\"blue accent-3\"\r\n                                    @click=\"createPDF()\"\r\n                                    v-on=\"on\">\r\n                                    <v-icon left>mdi-printer</v-icon>\r\n                                    Print Laporan\r\n                                </v-btn>\r\n                            </template>\r\n                            <span>Print</span>\r\n                        </v-tooltip>\r\n                    </v-flex>\r\n                </v-layout>\r\n\r\n                <div ref=\"table\" id=\"content\">\r\n                    <v-data-table \r\n                        sortable: false\r\n                        :headers=\"headers\" \r\n                        :items=\"laporans\" \r\n                        :search=\"keyword\" \r\n                        :loading=\"load\" \r\n                        v-if=\"!showable\"\r\n                        :items-per-page=\"12\"\r\n                        class=\"elevation-2\"\r\n                        hide-default-footer\r\n                        >\r\n                            <template v-slot:body=\"{ items }\">\r\n                                <tbody>\r\n                                    <tr v-for=\"(item,index) in items\" :key=\"item.id_bulan\">\r\n                                        <td>{{ index + 1 }}</td>\r\n                                        <td>{{ item.nama_bulan}}</td>\r\n                                        <td>{{ item.nama_produk}}</td>\r\n                                        <td>\r\n                                            <v-chip color=\"primary\" text-color=\"white\">{{ item.jumlah_produk }}</v-chip>\r\n                                        </td>\r\n                                    </tr>\r\n                                </tbody>\r\n                            </template>\r\n                    </v-data-table>\r\n                </div>\r\n            </v-container>\r\n        </v-card>\r\n        <v-snackbar v-model=\"snackbar\" :color=\"color\" :multi-line=\"true\" :timeout=\"3000\">\r\n            {{ text }}\r\n            <v-btn dark text @click=\"snackbar = false\">\r\n                Close\r\n            </v-btn>\r\n        </v-snackbar>\r\n    </v-container>\r\n</template>\r\n<script> \r\n//sweet alert\r\nimport Vue from 'vue' \r\nimport VueSweetalert2 from 'vue-sweetalert2';\r\nimport 'sweetalert2/dist/sweetalert2.min.css';\r\nVue.use(VueSweetalert2);\r\n//=====\r\nimport html2canvas from \"html2canvas\" // canvas\r\nimport jsPDF from 'jspdf' // bikin pdf\r\nimport 'jspdf-autotable'\r\nimport dropdown from 'vue-dropdowns';\r\nimport { log } from 'util'\r\nexport default {    \r\n    data () {       \r\n        return {       \r\n            //coba drop down\r\n            arrayOfObjects: [\r\n                { name: '2020' }\r\n            ],\r\n            object: {\r\n              name: '2020',\r\n            },\r\n            //=======\r\n            snackbar: false,\r\n            text: 'Hello, I\\'m a snackbar',\r\n            dialog: false,\r\n            showable: false,         \r\n            keyword: '',       \r\n            pagination:{\r\n                rowsPerPage: 12\r\n            },\r\n            headers: [             \r\n                {               \r\n                    text: 'No',               \r\n                    value: 'no',\r\n                    sortable: false          \r\n                },             \r\n                {               \r\n                    text: 'Bulan',               \r\n                    value: 'bulan',\r\n                    sortable: false            \r\n                },             \r\n                {               \r\n                    text: 'Nama Produk',               \r\n                    value: 'nama_produk',\r\n                    sortable: false\r\n                },            \r\n                {               \r\n                    text: 'Jumlah Penjualan',               \r\n                    value: 'jumlah_produk',\r\n                    fixed: true,\r\n                    sortable: false\r\n                }         \r\n            ],       \r\n            laporans: [],\r\n            nama_layanan: [],\r\n            snackbar: false,          \r\n            color: null,         \r\n            text: '',          \r\n            load: false, \r\n            form: {            \r\n                ukuran : '',           \r\n                harga : '',\r\n                created_at : '',\r\n                update_at : '',\r\n                tahun: '2020'\r\n            },         \r\n            nama_tahun:['2020'],\r\n            size : new FormData,         \r\n            typeInput: 'new',        \r\n            errors : '',         \r\n        }     \r\n    },  \r\n    //coba drop down   \r\n    components: {\r\n        'dropdown': dropdown,\r\n    },\r\n    methods:{ \r\n        succesPrinted(){\r\n            this.$swal('Sukses Print Laporan','','success');\r\n        },\r\n        convertBase64LocalImage(){\r\n        //add local image\r\n            let _this = this\r\n            var res\r\n            var xhr = new XMLHttpRequest();       \r\n            xhr.open(\"GET\", require('../../assets/header.png'), true); \r\n            xhr.responseType = \"blob\";\r\n            xhr.onload = function (e) {\r\n                    console.log(this.response);\r\n                    var reader = new FileReader();\r\n                    reader.onload = function(event) {\r\n                    res = event.target.result;\r\n                    _this.local_image = res \r\n                    }\r\n                    var file = this.response;\r\n                    reader.readAsDataURL(file)\r\n            }\r\n            xhr.send()\r\n        },\r\n        createPDF () {\r\n          let _this = this\r\n            console.log('=== local image ===')\r\n            console.log(this.local_image)\r\n            const header = this.local_image\r\n            // doc\r\n            const doc = new jsPDF('a4','px');\r\n            doc.addImage(header,'JPEG',25,20,400,110);\r\n            // isi auto table\r\n            //header\r\n            var headRows = function() {\r\n                return [{\r\n                no: \"ID\",\r\n                bulan: \"Bulan\",\r\n                nama_produk: \"Nama Produk\",\r\n                jumlah_penjualan: \"Jumlah Penjualan\",\r\n                }];\r\n            };\r\n            //body\r\n            var bodyRows = function() {\r\n                let body = [];\r\n                for(var i = 0, len = _this.laporans.length; i < len; i++){\r\n                body.push({\r\n                    no: i+1,\r\n                    bulan: _this.laporans[i].nama_bulan,\r\n                    nama_produk: _this.laporans[i].nama_produk,\r\n                    jumlah_penjualan: _this.laporans[i].jumlah_produk\r\n                });\r\n                }\r\n                return body;\r\n            }\r\n            //text judul\r\n            doc.setFontSize(20);\r\n            doc.setTextColor(40);\r\n            doc.setFontStyle('bold');\r\n            doc.text(\"Laporan Produk Terlaris\", 145, 150);\r\n            //text tahun\r\n            doc.setFontSize(15);\r\n            doc.setTextColor(40);\r\n            doc.setFontStyle('normal');\r\n            doc.text(\"Tahun : 2020\", 28, 180);\r\n            doc.autoTable({\r\n            margin: { top: 190 },\r\n            head: headRows(),\r\n            body: bodyRows()\r\n            })\r\n            //text cetak pada\r\n            doc.setFontSize(15);\r\n            doc.setTextColor(40);\r\n            doc.setFontStyle('normal');\r\n            var date = (new Date().toLocaleString('id',{month:'long', year:'numeric', day:'numeric'}))\r\n            doc.text(\"Dicetak Tanggal : \"+date, 28, 420);\r\n            doc.save('Laporan_Produk_Terlaris.pdf');\r\n            this.succesPrinted();\r\n        },\r\n        clickedButton(){\r\n            this.snackbar = true; //mengaktifkan snackbar               \r\n            this.color = 'green'; //memberi warna snackbar               \r\n            this.text = 'Clicked'; //memasukkan pesan ke snackbar               \r\n        },\r\n        methodToRunOnSelect(payload) {\r\n            this.object = payload;\r\n        },\r\n        getData(){             \r\n            var uri = this.$apiUrl + '/laporan/ProdukTerlaris?tahun=2020'             \r\n            this.$http.get(uri).then(response =>{                 \r\n                this.laporans=response.data.data           \r\n            })               \r\n        },         \r\n        sendData(){\r\n            this.tahun.append('tahun', this.form.ukuran);      \r\n            var uri =this.$apiUrl + '/laporan/LayananTerlaris/'             \r\n            this.load = true             \r\n            this.$http.post(uri, this.tahun).then(response =>{               \r\n                this.snackbar = true; //mengaktifkan snackbar               \r\n                this.color = 'green'; //memberi warna snackbar               \r\n                this.text = response.data.message; //memasukkan pesan ke snackbar               \r\n                this.load = false;               \r\n                this.dialog = false               \r\n                this.getData(); //mengambil data ukuran            \r\n                this.resetForm();           \r\n            }).catch(error =>{               \r\n                this.errors = error               \r\n                this.snackbar = true;               \r\n                this.text = 'Try Again';               \r\n                this.color = 'red';               \r\n                this.load = false;           \r\n            })         \r\n        }\r\n    },     \r\n    mounted(){         \r\n        this.getData();\r\n        this.convertBase64LocalImage();         \r\n        }, \r\n    } \r\n</script> \r\n<style lang=\"scss\" scoped>\r\n.content{\r\n    width: 85%;\r\n    margin: auto;\r\n}\r\n.v-btn {\r\n  color : white;\r\n}\r\n</style>"],"sourceRoot":"src/components/dashboardAdminContents"}]}